{
   "name":"Colorado State University Theme",
   "$schema":"https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.139.json",
   "dataColors":
      [
      "#1E4D2B",
      "#D9782D",
      "#9FAFA1",
      "#6B7B6E",
      "#004C6A",
      "#1D7D9D",
      "#C8C372",
      "#59595B",
      "#C9D845",
      "#CC5430",
      "#12A4B6",
      "#ECC530"
      ],
   "background":"#FFFFFF",
   "tableAccent":"#1E4D2B",
   "minimum": "#A5CBD8",
   "maximum": "#0F3F4F",
   "center": "#4A97B1",
   "bad":"#D64554",
   "textClasses":{
      "label":
         {
         "color":"#59595B",
         "fontSize":14,
         "fontFace": "Segoe UI"
         },
      "title":
         {
         "color":"#59595B",
         "fontSize":16,
         "fontFace": "Segoe UI Semibold"
         },
      "header":
         {
         "color":"#59595B",
         "fontFace": "Segoe UI"
         }
   },
   "visualStyles":{
      "*":{
         "*":{
            "background":[
               {
                  "transparency":0
               }
            ],
            "visualHeader":[
               {
               "foreground":{"solid":{"color":"#1E4D2B"}}
               }
            ],
            "outspacePane":[
               {
               "foregroundColor":{"solid":{"color":"#1E4D2B"}},
               "checkboxAndApplyColor":{"solid":{"color":"#9FAFA1"}}
               }
            ],
            "filterCard":[
               {
               "$id":"Applied",
               "foregroundColor":{"solid":{"color":"#59595B"}},
               "textSize":14
               },
               {
               "$id":"Available",
               "foregroundColor":{"solid":{"color":"#59595B"}},
               "textSize":14
               }
            ],
            "padding":[
               {
               "bottom": 5,
               "top": 5,
               "left": 5,
               "right": 5
               }
            ],
            "border": [
               {
               "color": {"solid":{"color": "#F0F4F9"}},
               "radius": 10,
               "show": true
               }
            ]
         }
      },
      "page":{
         "*":{
            "background":[
               {
                  "color":{"solid":{"color":"#FFFFFF"}},
                  "transparency":0
               }
            ],
            "pageSize": [
               {
                  "pageSizeTypes": "Custom",
                  "pageSizeHeight": 980,
                  "pageSizeWidth": 2000
               }
            ],
            "filterCard": [
               {
                  "backgroundColor":{"solid": {"color":"#E9E7C7"}},
                  "borderColor": {"solid": {"color": "#C8C372"}},
                  "border": true
               }
            ]
         }
      },
      "card": {
         "*":{
               "general": [
                  {
                  "keepLayerOrder": true
                  }
               ],
               "categoryLabels": [
                  {
                  "show": true
                  }
               ],
               "border": [
                  {
                  "show": true,
                  "color": {"solid": {"color": "#F0F4F9"}},
                  "radius": 10
                  }
               ],
               "background": [
                  {
                  "show": true,
                  "color":{"solid": {"color": "#F0F4F9"}},
                  "transparency": 0
                  }
               ],
               "padding": [
                  {
                  "left": 5,
                  "right": 5,
                  "bottom": 5,
                  "top": 5
                  }
               ],
               "labels": [
                  {
                  "fontSize": 32
                  }
               ]
         }
         
      },
      "tableEx": {
         "*":{
            "values": [
               {
               "fontColorPrimary": { "solid": { "color": "#003950"}},
               "backColorPrimary": { "solid": { "color": "#FFFFFF"}},
               "fontColorSecondary": { "solid": { "color": "#003950"}},
               "backColorSecondary": { "solid": { "color": "#F0F4F9"}},
               "outline": "Frame",
               "urlIcon": true,
               "wordWrap": true,
               "fontFamily": "Segoe UI",
               "fontSize": 12
               }
            ],
            "columnHeaders": [
               {
               "wordWrap": true,
               "autoSizeColumnWidth": false,
               "fontColor":{"solid": {"color": "#666666"}},
               "backColor":{"solid": {"color": "#F0F4F9"}}
               }
            ],
            "stylePreset": [{"name": "Minimal"}],
            "border": [
               {
               "color":{"solid": {"color": "#F0F4F9"}},
               "show": true,
               "radius": 10
               }
            ],
            "background": [
               {
               "show": true,
               "color":{"solid": {"color": "#F0F4F9"}}
                  }
            ],
            "padding": [
               {
                  "left": 2,
                  "right": 5,
                  "top": 5,
                  "bottom": 2
               }
            ]
         }
      },
      "shape": {
         "*": {
            "fill": [
               {
               "show": true,
               "fillColor": {"solid": {"color": "#F0F4F9"}},
               "transparency": 0
               }
            ],
            "border": [
               {
               "color": {"solid": {"color": "#F0F4F9"}},
               "radius": 10,
               "show": true
               }
            ],
            "background": [
               {
               "color": {"solid": {"color": "#FFFFFF"}},
               "show": false,
               "transparency": 0
               }
            ],
            "padding": [
               {
                  "left": 5,
                  "right": 5,
                  "top": 5,
                  "bottom": 5
               }
            ]
         }
      },
      "textbox": {
         "*": {
            "border": [
               {
                  "show": true,
                  "color": {"solid": {"color": "#1E4D2B"}},
                  "radius": 10
               }
            ],
            "padding": [
               {
                  "left": 5,
                  "right": 5,
                  "top": 5,
                  "bottom": 5
               }
            ],
            "text": [
               {
                  "fontFamily": "Segoe UI",
                  "fontSize": 14,
                  "color": {"solid": {"color": "#666666"}}
               }
            ],
            "background": [
               {
                  "color": {"solid": {"color": "#F0F4F9"}},
                  "transparency": 0
               }
            ],
            "title": [
               {
                  "fontFamily": "Segoe UI Semibold",
                  "fontSize": 18,
                  "fontColor": {"solid": {"color": "#666666"}}
               }
            ]
         }
      },
      "pivotTable": {
         "*": {
            "stylePreset": [
               {
                  "stylePreset": "Minimal"
               }
            ],
            "grid": [
               {
                  "gridVertical": true,
                  "gridVerticalColor": {"solid": {"color": "#D8D8DF"}},
                  "gridVerticalWeight": 2,
                  "gridHorizontal": true,
                  "gridHorizontalColor": {"solid": {"color": "#D8D8DF"}},
                  "gridHorizontalWeight": 2,
                  "rowPadding": 3,
                  "outlineWeight": 1,
                  "outlineColor": {"solid": {"color": "#1E4D2B"}},
                  "textSize": 12
               }
            ],
            "columnHeaders": [
               {
                  "fontColor": {"solid": {"color": "#252423"}},
                  "backColor": {"solid": {"color": "#D9DFD9"}},
                  "alignment": "Center",
                  "titleAlignment": "Center",
                  "autoSizeColumnWidth": false,
                  "wordWrap": true,
                  "fontFamily": "Segoe UI Semibold",
                  "fontSize": 13
               }
            ],
            "rowHeaders": [
               {
                  "fontColor": {"solid": {"color": "#252423"}},
                  "backColor": {"solid": {"color": "#F0F4F9"}},
                  "alignment": "Left",
                  "autoSizeColumnWidth": false,
                  "urlIcon": false,
                  "wordWrap": true,
                  "fontFamily": "Segoe UI Semibold",
                  "fontSize": 12
               }
            ],
            "border": [
               {
                  "show": true,
                  "color": {"solid": {"color": "#F0F4F9"}},
                  "radius": 10
               }
            ],
            "values": [
               {
                  "fontColorPrimary": {"solid": {"color": "#252423"}},
                  "fontColorSecondary": {"solid": {"color": "#252423"}},
                  "backColorPrimary": {"solid": {"color": "#F0F4F9"}},
                  "backColorSecondary": {"solid": {"color": "#FFFFFF"}},
                  "wordWrap": true,
                  "fontFamily": "Segoe UI",
                  "fontSize": 12,
                  "valuesOnRow": true
               }
            ],
            "subTotals": [
               {
                  "fontFamily": "Segoe UI",
                  "fontSize": 12,
                  "fontColor": {"solid": {"color": "#252423"}},
                  "rowSubtotalsLabel": "Total",
                  "rowSubtotalsPosition": "Bottom",
                  "perRowLevel": false
               }
            ],
            "rowTotal": [
               {
                  "fontFamily": "Segoe UI",
                  "fontSize": 12,
                  "fontColor": {"solid": {"color": "#252423"}},
                  "backColor": {"solid": {"color": "#A5B8AA"}}

               }
            ],
            "columnTotal": [
               {
                  "fontFamily": "Segoe UI",
                  "fontSize": 12,
                  "fontColor": {"solid": {"color": "#252423"}},
                  "backColor": {"solid": {"color": "#A5B8AA"}}
               }
            ]
         }
      }
   }
}