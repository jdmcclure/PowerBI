{
   "name":"Colorado State University Theme",
   "$schema":"https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.126.json",
   "dataColors":
      [
      "#1E4D2B",
      "#D9782D",
      "#9FAFA1",
      "#6B7B6E",
      "#004C6A",
      "#1D7D9D",
      "#C8C372",
      "#59595B",
      "#C9D845",
      "#CC5430",
      "#12A4B6",
      "#ECC530"
      ],
   "background":"#F0F4F9",
   "tableAccent":"#1E4D2B",
   "minimum": "#A5CBD8",
   "maximum": "#0F3F4F",
   "center": "#4A97B1",
   "bad":"#D64554",
   "textClasses":{
      "label":
         {
         "color":"#59595B",
         "fontSize":12,
         "fontFace": "Segoe UI"
         },
      "title":
         {
         "color":"#59595B",
         "fontSize":14,
         "fontFace": "Segoe UI Semibold"
         },
      "header":
         {
         "color":"#59595B",
         "fontFace": "Segoe UI"
         }
   },
   "visualStyles":{
      "*":{
         "*":{
            "background":[
               {
                  "transparency":0
               }
            ],
            "visualHeader":[
               {
               "foreground":{"solid":{"color":"#1E4D2B"}}
               }
            ],
            "outspacePane":[
               {
               "foregroundColor":{"solid":{"color":"#1E4D2B"}},
               "checkboxAndApplyColor":{"solid":{"color":"#9FAFA1"}}
               }
            ],
            "filterCard":[
               {
               "$id":"Applied",
               "foregroundColor":{"solid":{"color":"#59595B"}},
               "textSize":11
               },
               {
               "$id":"Available",
               "foregroundColor":{"solid":{"color":"#59595B"}},
               "textSize":11
               }
            ],
            "padding":[
               {
               "bottom": 5,
               "top": 5,
               "left": 5,
               "right": 5
               }
            ],
            "border": [
               {
               "color": {"solid":{"color": "#F0F4F9"}},
               "radius": 10,
               "show": true
               }
            ]
         }
      },
      "page":{
         "*":{
            "background":[
               {
                  "color":{"solid":{"color":"#FFFFFF"}},
                  "transparency":0
               }
            ]
         }
      },
      "card": {
         "*":{
               "general": [
                  {
                  "keepLayerOrder": true
                  }
               ],
               "categoryLabels": [
                  {
                  "show": true
                  }
               ],
               "border": [
                  {
                  "show": true,
                  "color": {"solid": {"color": "#F0F4F9"}},
                  "radius": 10
                  }
               ],
               "background": [
                  {
                  "show": true,
                  "color":{"solid": {"color": "#F0F4F9"}},
                  "transparency": 0
                  }
               ],
               "padding": [
                  {
                  "left": 5,
                  "right": 5,
                  "bottom": 5,
                  "top": 5
                  }
               ],
               "labels": [
                  {
                  "fontSize": 28
                  }
               ]
         }
         
      },
      "tableEx": {
         "*":{
            "values": [
               {
               "fontColorPrimary": { "solid": { "color": "#003950"}},
               "backColorPrimary": { "solid": { "color": "#FFFFFF"}},
               "fontColorSecondary": { "solid": { "color": "#003950"}},
               "backColorSecondary": { "solid": { "color": "#F0F4F9"}},
               "outline": "Frame",
               "urlIcon": true,
               "wordWrap": true,
               "fontFamily": "Segoe UI",
               "fontSize": 11
               }
            ],
            "columnHeaders": [
               {
               "wordWrap": true,
               "autoSizeColumnWidth": false,
               "fontColor":{"solid": {"color": "#666666"}},
               "backColor":{"solid": {"color": "#F0F4F9"}}
               }
            ],
            "stylePreset": [{"name": "Minimal"}],
            "border": [
               {
               "color":{"solid": {"color": "#F0F4F9"}},
               "show": true,
               "radius": 10
               }
            ],
            "background": [
               {
               "show": true,
               "color":{"solid": {"color": "#F0F4F9"}}
                  }
            ],
            "padding": [
               {
                  "left": 2,
                  "right": 5,
                  "top": 5,
                  "bottom": 2
               }
            ]
         }
      },
      "shape": {
         "*": {
            "fill": [
               {
               "show": true,
               "fillColor": {"solid": {"color": "#F0F4F9"}},
               "transparency": 0
               }
            ],
            "border": [
               {
               "color": {"solid": {"color": "#F0F4F9"}},
               "radius": 10,
               "show": true
               }
            ],
            "background": [
               {
               "color": {"solid": {"color": "#FFFFFF"}},
               "show": false,
               "transparency": 0
               }
            ],
            "padding": [
               {
                  "left": 5,
                  "right": 5,
                  "top": 5,
                  "bottom": 5
               }
            ]
         }
      },
      "textbox": {
         "*": {
            "border": [
               {
                  "show": true,
                  "color": {"solid": {"color": "#1E4D2B"}},
                  "radius": 10
               }
            ],
            "padding": [
               {
                  "left": 5,
                  "right": 5,
                  "top": 5,
                  "bottom": 5
               }
            ],
            "text": [
               {
                  "fontFamily": "Segoe UI",
                  "fontSize": 12,
                  "color": {"solid": {"color": "#666666"}}
               }
            ],
            "background": [
               {
                  "color": {"solid": {"color": "#F0F9F9"}},
                  "transparency": 0
               }
            ],
            "title": [
               {
                  "fontFamily": "Segoe UI Semibold",
                  "fontSize": 14,
                  "fontColor": {"solid": {"color": "#666666"}}
               }
            ]
         }
      },
      "actionButton": {
         "*": {
            "border": [
               {
                  "show": true,
                  "color": {"solid": {"color": "#1E4D2B"}},
                  "radius": 5
               }
            ],
            "background": [
               {
                  "show": false
               }
            ],
            "fill": [
               {
                  "fillColor": {"solid": {"color": "#1E4D2B"}}
               }
            ],
            "padding": [
               {
                  "left": 0,
                  "right": 0,
                  "top": 0,
                  "bottom": 0
               }
            ],
            "text": [
               {
                  "verticalAlignment": "middle",
                  "horizontalAlignment": "Center",
                  "fontFamily": "Segoe UI",
                  "fontSize": 10,
                  "fontColor": {"solid": {"color": "#666666"}}
               }
            ]
         }
      }
   }
}